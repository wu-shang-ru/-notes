                    var isTargetBlank = (jsonObj[i].LinkTarget.toUpperCase() == 'Y') ? ' target="_blank"' : '';

                    sHtml[sHtml.length] = '<li><a href="'+ jsonObj[i].Link +'" '+ isTargetBlank +'><p>'+ jsonObj[i].Text +'</p></a></li>';
                }
                $('.menu_'+siteIDtoLowerCase+'-cautcates').append(sHtml.join(''));
            };

        };

